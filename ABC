package ALPHA_BATCH;

public class hi {
    public static void main(String[] args) {
         int nums [] = {1,4,0,7,5,2,0};
        
        
    }
    public static void Movezeros(int []nums){
        int count = 0;
        for (int i = 0;i<nums.length;i++){
            if (nums[i]!=0){
                nums[count++] = nums[i];
            }

        }
        while (count<nums.length){
            nums[count++] = 0;
        }
    }
}
